<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="fall2018.csc2017.slidingtiles">

    <application
        android:name=".App"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.Light.NoActionBar">
        <activity android:name=".WelcomeActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".StartingActivity" />
        <activity android:name=".LoginActivity" />
        <activity android:name=".PlaySlidingTilesActivity" />
        <activity android:name=".PlayPegSolitaireActivity" />
        <activity android:name=".PlayMemoryPuzzleActivity" />
        <activity android:name=".GameCentreActivity" />
        <activity android:name=".ScoreBoardActivity" />
        <activity android:name=".UserProfileActivity" />
        <activity android:name=".SignUpActivity" />
        <activity android:name=".SetUpActivity" />
        <activity android:name=".MemoryGameSetUpActivity" />
    </application>

</manifest>